@model MailApp.Models.Groups.RemoveMemberViewModel

<form asp-action="RemoveMember">
    <input asp-for="GroupId" type="hidden" />
    <label asp-for="AccountNick"></label>
    <div class="input-group mb-3">
        <input asp-for="AccountNick" class="form-control" placeholder="Group name">
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>